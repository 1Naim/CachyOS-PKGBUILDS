
pkgname=cachyos-v4-mirrorlist
pkgver=4
pkgrel=1
pkgdesc="cachyos-v3-mirrorlist"
arch=('any')
license=('GPL3')
groups=(cachyos)
options=(!strip !emptydirs)
backup=("etc/pacman.d/$pkgname")  # to keep possibly ranked mirrorlist
source=(cachyos-v4-mirrorlist)
sha512sums=('f54e21a724d3258705f8f8a060bf038c6518c0881020db3b28ff2a2602a852180366ae43c06abed20f98ed4e24b70b7e40d025ef0a9727e7641add8cc036a721')
package() {
 install -Dm644 "$srcdir/$pkgname" "$pkgdir/etc/pacman.d/$pkgname"
}
