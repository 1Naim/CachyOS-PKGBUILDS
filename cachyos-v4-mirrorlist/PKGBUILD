
pkgname=cachyos-v4-mirrorlist
pkgver=7
pkgrel=1
pkgdesc="cachyos-v4-mirrorlist"
arch=('any')
license=('GPL3')
groups=(cachyos)
options=(!strip !emptydirs)
backup=("etc/pacman.d/$pkgname")  # to keep possibly ranked mirrorlist
source=(cachyos-v4-mirrorlist)
sha512sums=('19ffd7c5d30458abaad7759468e7282a62223a20573d86561cc35c2bc9fea5693c421488999380edef355242272f479dcadcd3c4b0859ab8e84451f2d518644a')
package() {
 install -Dm644 "$srcdir/$pkgname" "$pkgdir/etc/pacman.d/$pkgname"
}
