
pkgname=cachyos-v4-mirrorlist
pkgver=3
pkgrel=1
pkgdesc="cachyos-v3-mirrorlist"
arch=('any')
license=('GPL3')
groups=(cachyos)
options=(!strip !emptydirs)
backup=("etc/pacman.d/$pkgname")  # to keep possibly ranked mirrorlist
source=(cachyos-v4-mirrorlist)
sha512sums=('d9870de6050f3d736e6a261d2b5d370dbdcd668d2f636ff60ba3099c9bc7c33a5573849a927819294131f7c6f763fcf967fda26593f107f123c5d5f10fec8dcc')
package() {
 install -Dm644 "$srcdir/$pkgname" "$pkgdir/etc/pacman.d/$pkgname"
}
