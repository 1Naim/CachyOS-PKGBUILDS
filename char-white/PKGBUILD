pkgname=char-white
pkgdesc="Char-White Icons"
pkgver=r3.ef1c007
pkgrel=1
arch=('any')
url="https://github.com/CachyOS/char-white.git"
license=('GPL')
makedepends=('git')
source=("git+https://github.com/CachyOS/char-white.git")
sha256sums=('SKIP')
groups=('cachyos')

pkgver() {
	cd "$srcdir/$pkgname"
	printf "r%s.%s" "$(git rev-list --count HEAD)" "$(git rev-parse --short HEAD)"
}

package() {
    install -d $pkgdir/usr
    cp -rf $srcdir/$pkgname/usr $pkgdir
}
