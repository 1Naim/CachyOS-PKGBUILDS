
pkgname=cachyos-mirrorlist
pkgver=19
pkgrel=1
pkgdesc="cachyos mirrorlist"
arch=('any')
license=(GPL-3.0-or-later)
groups=(cachyos)
options=(!strip !emptydirs)
backup=("etc/pacman.d/$pkgname")  # to keep possibly ranked mirrorlist
source=(cachyos-mirrorlist)
sha512sums=('b1f7404b2f45613e3041de69f63484b8a7361b737d4cea2248f5d854e2071d3e635e54cb1cf31822a4431746bfd39635d986416b0af39eb6bd178b5b3ff109ee')
package() {
 #install -d "$pkgdir/etc/pacman.d"
 install -Dm644 "$srcdir/$pkgname" "$pkgdir/etc/pacman.d/$pkgname"
}
