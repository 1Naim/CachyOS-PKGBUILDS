
pkgname=cachyos-v3-mirrorlist
pkgver=6
pkgrel=1
pkgdesc="cachyos-v3-mirrorlist"
arch=('any')
license=('GPL3')
options=(!strip !emptydirs)
backup=("etc/pacman.d/$pkgname")  # to keep possibly ranked mirrorlist
source=(cachyos-v3-mirrorlist)
sha512sums=('1ee3df10016a6336fe623d331959c773bcb919092972753bfd70a4a6074e54c504d917e817b2b475c360b84e95a77bb0cf9f5ffc8df0aae684e8c962077e4541')
package() {
 #install -d "$pkgdir/etc/pacman.d"
 install -Dm644 "$srcdir/$pkgname" "$pkgdir/etc/pacman.d/$pkgname"
}
