pkgname=cachyos-settings
pkgver=1
pkgrel=1
arch=('x86_64')
license=('GPL')
makedepends=('coreutils')
pkgdesc='CachyOS - Settings'
source=('etc.tar.gz')
install=$pkgname.install
sha256sums=('SKIP')
depends=(
    'irqbalance'
    'zramd'   
    'reflector'
    'haveged'
    'profile-sync-daemon')
package() {
    install -d $pkgdir/etc

    cp -rf $srcdir/etc $pkgdir

}


